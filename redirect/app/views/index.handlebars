<!-- Modal signin -->
<div class="modal fade modal_car" id="signin" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h2 class="modal-title"><span class="big-ico auth"></span> Sign in</h2>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-6">
                            <br/>
                            <br/>
                            <form class="form-horizontal" id="signin-form" role="form" method="post">
                                <div class="form-group">
                                    <div class="inner-addon left-addon" style="color: #b1b1b1">
                                        <div class="input-group signin_input">
                                            <i class="glyphicon icon-store"></i>
                                            <input type="text" class="form-control icon" placeholder="Your store" data-remote="/validate/tenant" name="subdomain" data-error="No account exists for this domain" data-domain='{{domain}}' data-protocol='{{protocol}}' required>
                                            <div class="input-group-addon login"><span class="point">.</span>tradedepot.co</div>
                                        </div>
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>
                                <br/>
                                <div class="form-group signin_email">
                                    <div class="inner-addon left-addon" style="color: #b1b1b1">
                                        <i class="glyphicon icon-user"></i>
                                        <input type="email" class="form-control signin_email icon" placeholder="Email address" data-error="Please enter a valid email" name="email" required>
                                        <i class="glyphicon icon-pass signin_paswword"></i>
                                        <input type="password" data-minlength="6" class="form-control signin_paswword icon" placeholder="Password" name="password" required>
                                    </div>
                                    <div class="help-block with-errors"></div>
                                </div>
                                <br/>
                                <div class="form-group checkbox">
                                    <label>
                                        <input type="checkbox" name="chkRemember"> Remember
                                    </label>
                                </div>
                                <br/>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-primary" style="padding: 5px 55px;text-transform: uppercase;">Log in</button>
                                </div>
                                <br/>
                            </form>

                        </div>
                        <div class="col-sm-6 col-sm-offset-0 hidden-xs" style="padding: 0;">
                            <div id="animation" class="hidden-xs animation"></div>
                        </div>
                        <div class="col-sm-12">
                            <div class="row footer_modal">
                                <a href="#forgot" class="pull-left" data-dismiss="modal" data-toggle="modal">Forgot Your Password?</a>
                                <p class="text-center pull-right">
                                    Not a client? <a href="/signup">Sign up</a>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

